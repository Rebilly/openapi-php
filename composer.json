{
  "name": "rebilly/openapi-php",
  "description": "OpenAPI Specification SDK for PHP",
  "license": "MIT",
  "keywords": [],
  "homepage": "https://github.com/rebilly/openapi-php/",
  "authors": [
    {
      "name": "Veaceslav Medvedev",
      "email": "slavcopost@gmail.com",
      "role": "Developer"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.1",
    "ext-json": "*",
    "psr/http-message": "^1.0",
    "justinrainbow/json-schema": "~5.2",
    "phpunit/phpunit": "~7.5"
  },
  "autoload": {
    "psr-4": {
      "Rebilly\\OpenAPI\\": ["src"]
    }
  },
  "require-dev": {
    "guzzlehttp/guzzle": "~6.2",
    "scrutinizer/ocular": "^1.5.2",
    "friendsofphp/php-cs-fixer": "^2.15"
  },
  "autoload-dev": {
    "psr-4": {
      "Rebilly\\OpenAPI\\": ["tests"]
    }
  },
  "suggest": {
    "guzzlehttp/guzzle": "~6.2"
  },
  "scripts": {
    "cs": "./vendor/bin/php-cs-fixer fix --verbose --dry-run",
    "cs-fix": "./vendor/bin/php-cs-fixer fix --verbose",
    "test-wip": "./vendor/bin/phpunit --group=wip",
    "test": "./vendor/bin/phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
